{
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
      {
      "source": "/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes" :[
         {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml; charset=utf-8"
      }
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8"
      }
    },
  ]
}